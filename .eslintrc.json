{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "eol-last": ["error", "never"]
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": "off",
        "indent": "off",
        "eol-last": "off",
        "no-underscore-dangle": "off"
    }
}
